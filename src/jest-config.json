{
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "collectCoverageFrom": [
      "**/*.{js}"
    ],
    "coveragePathIgnorePatterns": [
      "node_modules",
      "(.mock)\\.(js)$"
    ],
    "testMatch": [
      "**/*/*.spec.(js)"
    ],
    "coverageDirectory": "../coverage/",
    "cacheDirectory": "/var/tmp/jest_cache",
    "coverageThreshold": {
      "global": {
        "lines": 0,
        "statements": 0,
        "functions": 0,
        "branches": 0,
        "statments": 0
      }
    }
  }
  